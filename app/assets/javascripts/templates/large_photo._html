<h2><%= name %></h2>
<img src="<%= largeUrl %>" width="<%= largeWidth %>" height="<%= largeHeight %>" alt="<%= name %>" title="<%= name %>"/>
<div>
  <%= description %>
  <a href="<%= rawUrl %>">Original</a>
  <div class="tags">
    Tags:
    <ul>
      <% _(tags).each(function (tag) { %>
        <li><%= tag %><% if (PhotosApp.currentUser.canManageTags()) { %><a href="#" class="remove_tag">x</a><% } %></li>
      <% }); %>
    </ul>
    <% if (PhotosApp.currentUser.canManageTags()) { %>
      <a href="#" class="add_tag">+</a>
    <% } %>
  </div>
</div>
<% if (PhotosApp.currentUser.canManagePhotos()) { %>
  <div class="administration">
    <a href="delete" class="delete">delete</a>
  </div>
<% } %>
